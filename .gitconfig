[core]
attributesfile = .gitattributes

[filter "redact-env"]
	clean = sed -E \
		-e 's/^IP_DOMAIN=.*/IP_DOMAIN=example.com/g' \
		-e 's/192\\.168\\.[0-9]+\\.[0-9]+/192.168.XXX.XXX/g' \
		-e 's/^APP_KEY=.*/APP_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX/g'
	smudge = cat

[filter "redact-compose"]
	clean = sed -E \
		-e 's/192\\.168\\.[0-9]+\\.[0-9]+/192.168.XXX.XXX/g'
	smudge = cat